{
  "name": "outline-server",
  "private": true,
  "devDependencies": {
    "@types/jasmine": "^2.5.53",
    "clang-format": "^1.2.2",
    "husky": "^1.3.1",
    "jasmine": "^2.6.0",
    "tslint": "^5.9.1",
    "typescript": "^3.2.2"
  },
  "engines": {
    "yarn": ">=1.0"
  },
  "scripts": {
    "clean": "rm -rf src/server_manager/bower_components/ src/*/node_modules/ build/ node_modules/ src/server_manager/install_scripts/do_install_script.ts",
    "do": "bash ./scripts/do_action.sh"
  },
  "workspaces": [
    "src/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "for i in . src/server_manager/electron_app src/metrics_server src/shadowbox; do tslint -p $i --fix; done; git-clang-format"
    }
  }
}
