{
  "name": "outline-server",
  "private": true,
  "devDependencies": {
    "@bazel/bazel": "latest",
    "@bazel/buildifier": "latest",
    "@bazel/ibazel": "latest",
    "@bazel/jasmine": "^0.35.0",
    "@bazel/typescript": "latest",
    "@types/jasmine": "^2.5.53",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.6.9",
    "@types/randomstring": "^1.1.6",
    "@types/restify": "2",
    "clang-format": "^1.2.2",
    "husky": "^1.3.1",
    "jasmine": "^2.6.0",
    "tslint": "^5.9.1",
    "typescript": "~3.4.0"
  },
  "engines": {
    "yarn": ">=1.0"
  },
  "scripts": {
    "clean": "rm -rf src/server_manager/bower_components/ src/*/node_modules/ build/ node_modules/ src/server_manager/install_scripts/do_install_script.ts",
    "do": "bash ./scripts/do_action.sh",
    "build": "bazel build //...",
    "test": "bazel test //..."
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn tslint --fix 'src/**/*.ts' -e '**/bower_components/**' -e '**/node_modules/**' && yarn git-clang-format"
    }
  },
  "dependencies": {
    "js-yaml": "^3.13.1",
    "mkdirp": "^0.5.1",
    "ShadowsocksConfig": "Jigsaw-Code/outline-shadowsocksconfig",
    "randomstring": "^1.1.5",
    "request-lite": "^2.40.1",
    "restify": "4",
    "uuid": "^3.3.2"
  }
}
